{
	"name": "mongodm",
	"version": "1.0.0",
	"author": "Damien Marchand",
	"license": "MIT",
	"repository": "",
	"keywords": [
		"mongodb",
		"typescript",
		"node",
		"orm"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=8.10"
	},
	"engineStrict": true,
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublishOnly": "yarn build",
		"release": "standard-version",
		"release:first": "standard-version --first-release",
		"test": "jest --forceExit --detectOpenHandles",
		"test:watch": "jest --watch --verbose false",
		"example": "ts-node-dev examples/index.ts",
		"dev": "jest --detectOpenHandles -- src/decorators/tests/*.decorator.test.ts"
	},
	"dependencies": {
		"lodash": "^4.17.20",
		"mongodb": "^3.6.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.6.3"
	},
	"devDependencies": {
		"@types/faker": "^5.1.5",
		"@types/jest": "^26.0.20",
		"@types/lodash": "^4.14.168",
		"@types/mongodb": "^3.5.26",
		"@types/node": "^14.14.21",
		"jest": "^26.6.3",
		"rimraf": "^3.0.0",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.1",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.1.3",
		"typescript-tslint-plugin": "^1.0.1"
	}
}
